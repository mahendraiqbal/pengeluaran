{
  "name": "Pengeluaran Bulanan",
  "short_name": "Pengeluaran",
  "description": "Aplikasi pencatatan pengeluaran bulanan",
  "start_url": "/dashboard",
  "display": "standalone",
  "background_color": "#fef6e4",
  "theme_color": "#8bd3dd",
  "icons": [
    {
      "src": "/icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "image",
          "accept": ["image/*"]
        }
      ]
    }
  }
}
